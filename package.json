{
  "name": "sf_dashboard",
  "version": "1.0.0",
  "description": "This project implements a comprehensive data pipeline for San Francisco municipal data, facilitating the flow from CSV files and Google Sheets into a structured Postgres database with landing, staging, and core layers, followed by reverse ETL back to Google Sheets for analysis and reporting.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "tsx src/server/bootstrap.ts",
    "start": "node ./dist/src/server/bootstrap.js",
    "test:unit": "vitest run --project unit",
    "test:contracts": "vitest run --project contracts --passWithNoTests",
    "test:integration": "vitest run --project integration --passWithNoTests",
    "test:arch": "vitest run --project arch",
    "test": "vitest run --project unit",
    "test:all": "pnpm -s test:unit && pnpm -s test:contracts && pnpm -s test:integration && pnpm -s test:arch",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:ci": "eslint . --max-warnings=0",
    "ci:verify": "pnpm -s typecheck && pnpm -s lint && pnpm -s test:arch && pnpm -s test && pnpm -s build",
    "lint:changed": "git diff --cached --name-only --diff-filter=d | grep -E '\\.(js|jsx|ts|tsx)$' | xargs --no-run-if-empty eslint --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "start:ingest": "tsx services/ingest/csvLoader.ts",
    "run:evictions:landing": "tsx services/ingest/csvLoader.ts",
    "run:evictions:landing:write": "DRY_RUN=false tsx services/ingest/csvLoader.ts",
    "run:evictions:staging": "tsx services/transforms/evictionsTransform.ts",
    "run:evictions:core": "tsx services/transforms/evictionsToCore.ts",
    "registry:sf": "node scripts/build-datasf-index.mjs",
    "registry:socrata:sf": "node scripts/build-datasf-index.mjs --domain=data.sfgov.org --out=municipalities/CA/SF/directory.json --tokenEnv=SOCRATA_APP_ID",
    "registry:socrata:detroit": "node scripts/build-datasf-index.mjs --domain=data.detroitmi.gov --out=municipalities/MI/Detroit/directory.json --tokenEnv=SOCRATA_APP_ID",
    "registry:ckan:sandiego": "node scripts/build-ckan-index.mjs --base=https://data.sandiego.gov --out=municipalities/CA/SanDiego/directory.ckan.json --tokenEnv=SAN_DIEGO_CKAN_TOKEN",
    "registry:arcgis:example": "node scripts/build-arcgis-index.mjs --portal=https://www.arcgis.com --out=municipalities/ARCGIS/directory.arcgis.json --query=type:(\"Feature Service\" OR \"Feature Layer\") AND owner:esri",
    "registry:refresh": "npm run registry:socrata:sf && npm run registry:socrata:detroit",
    "registry:merge": "node scripts/merge-registries.mjs",
    "registry:load:pg": "node scripts/registry-to-pg.mjs --files=municipalities/CA/SF/directory.json,municipalities/MI/Detroit/directory.json",
    "registry:all": "npm run registry:refresh && npm run registry:merge && npm run registry:load:pg",
    "env:local:init": "node scripts/env-local-init.mjs",
    "env:validate": "node scripts/env-validate.mjs",
    "check:env-import": "node scripts/check-env-import.mjs",
    "secret-scan": "sh -c 'command -v gitleaks >/dev/null 2>&1 && gitleaks detect --no-git --redact -c .gitleaks.toml || (echo \"gitleaks not found. Install: brew install gitleaks\" >&2; exit 1)'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/eslint": "^9.0.0",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@types/pg-copy-streams": "^1.2.5",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-regexp": "^2.0.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "fast-csv": "^5.0.5",
    "nanoid": "^5.0.7",
    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0",
    "pino": "^9.9.4",
    "zod": "^4.1.5"
  }
}
