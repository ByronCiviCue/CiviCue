{
  "name": "sf_dashboard",
  "version": "1.0.0",
  "description": "This project implements a comprehensive data pipeline for San Francisco municipal data, facilitating the flow from CSV files and Google Sheets into a structured Postgres database with landing, staging, and core layers, followed by reverse ETL back to Google Sheets for analysis and reporting.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w .",
    "start:ingest": "ts-node services/ingest/csvLoader.ts",
    "run:evictions:landing": "ts-node services/ingest/csvLoader.ts",
    "run:evictions:landing:write": "DRY_RUN=false ts-node services/ingest/csvLoader.ts",
    "run:evictions:staging": "ts-node services/transforms/evictionsTransform.ts",
    "run:evictions:core": "ts-node services/transforms/evictionsToCore.ts",
    "registry:sf": "node scripts/build-datasf-index.mjs",
    "registry:socrata:sf": "node scripts/build-datasf-index.mjs --domain=data.sfgov.org --out=municipalities/CA/SF/directory.json --tokenEnv=SOCRATA_APP_ID",
    "registry:socrata:detroit": "node scripts/build-datasf-index.mjs --domain=data.detroitmi.gov --out=municipalities/MI/Detroit/directory.json --tokenEnv=SOCRATA_APP_ID",
    "registry:ckan:sandiego": "node scripts/build-ckan-index.mjs --base=https://data.sandiego.gov --out=municipalities/CA/SanDiego/directory.ckan.json --tokenEnv=SAN_DIEGO_CKAN_TOKEN",
    "registry:arcgis:example": "node scripts/build-arcgis-index.mjs --portal=https://www.arcgis.com --out=municipalities/ARCGIS/directory.arcgis.json --query=type:(\"Feature Service\" OR \"Feature Layer\") AND owner:esri",
    "registry:refresh": "npm run registry:socrata:sf && npm run registry:socrata:detroit",
    "registry:merge": "node scripts/merge-registries.mjs",
    "registry:load:pg": "node scripts/registry-to-pg.mjs --files=municipalities/CA/SF/directory.json,municipalities/MI/Detroit/directory.json",
    "registry:all": "npm run registry:refresh && npm run registry:merge && npm run registry:load:pg"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.5",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "fast-csv": "^5.0.5",
    "nanoid": "^5.0.7",
    "pg": "^8.16.3",
    "pg-copy-streams": "^7.0.0",
    "pino": "^9.9.4",
    "zod": "^4.1.5"
  }
}
